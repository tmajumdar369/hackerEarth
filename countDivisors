import java.util.Scanner;

public class countDivisor {
    public static void main(String arsgsp[]){
        Scanner sc=new Scanner(System.in);
        int k=sc.nextInt();
        int l=sc.nextInt();
        int m=sc.nextInt();
        int c=0;
        for(int i=k;i<=l;i++){
            if(i%m==0)
                c++;
        }
        System.out.println(c);
    }
}
Language: Java8 Time:0.4067s Space: 64
