
import java.util.*;
public class productOfNos {
    public static void main(String args[]){
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        long s=1;
        for(int i=0;i<n;i++){
            int x=sc.nextInt();
            s=(s*x)%(1000000000+7);
        }
        System.out.println(s);
    }
}
